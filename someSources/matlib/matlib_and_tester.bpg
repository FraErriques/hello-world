#------------------------------------------------------------------------------
VERSION = BWS.01
#------------------------------------------------------------------------------
!ifndef ROOT
ROOT = $(MAKEDIR)\..
!endif
#------------------------------------------------------------------------------
MAKE = $(ROOT)\bin\make.exe -$(MAKEFLAGS) -f$**
DCC = $(ROOT)\bin\dcc32.exe $**
BRCC = $(ROOT)\bin\brcc32.exe $**
#------------------------------------------------------------------------------
PROJECTS = matlib test_console
#------------------------------------------------------------------------------
default: $(PROJECTS)
#------------------------------------------------------------------------------

matlib: matlib.bpr
  $(MAKE)

test_console: test_console.bpr
  $(MAKE)


